{
    "delay": "500",
    "events": {
        "restart": "osascript -e 'display notification \"app restarted\" with title \"nodemon\"'"
    },
    "ignore": [
        ".git",
        "node_modules",
        "*.test.js"
    ],
    "watch": [
        "node/src"
    ],
    "execMap": {
        "js": "node --inspect"
    }
}
